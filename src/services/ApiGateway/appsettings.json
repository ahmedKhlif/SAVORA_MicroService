{
  "Logging": {
    "LogLevel": {
      "Default": "Information",
      "Microsoft.AspNetCore": "Warning",
      "Yarp": "Information"
    }
  },
  "AllowedHosts": "*",
  "Urls": "http://localhost:5010",
  "Serilog": {
    "MinimumLevel": {
      "Default": "Information",
      "Override": {
        "Microsoft": "Warning",
        "System": "Warning",
        "Yarp": "Information"
      }
    }
  },
  "ReverseProxy": {
    "Routes": {
      "auth-route": {
        "ClusterId": "auth-cluster",
        "Match": {
          "Path": "/api/auth/{**catch-all}"
        },
        "Transforms": [
          { "PathRemovePrefix": "/api/auth" },
          { "PathPrefix": "/api/auth" }
        ]
      },
      "articles-route": {
        "ClusterId": "articles-cluster",
        "Match": {
          "Path": "/api/articles/{**catch-all}"
        }
      },
      "parts-route": {
        "ClusterId": "articles-cluster",
        "Match": {
          "Path": "/api/parts/{**catch-all}"
        }
      },
      "reclamations-route": {
        "ClusterId": "reclamations-cluster",
        "Match": {
          "Path": "/api/reclamations/{**catch-all}"
        }
      },
      "clients-route": {
        "ClusterId": "reclamations-cluster",
        "Match": {
          "Path": "/api/clients/{**catch-all}"
        }
      },
      "notifications-route": {
        "ClusterId": "reclamations-cluster",
        "Match": {
          "Path": "/api/notifications/{**catch-all}"
        }
      },
      "dashboard-route": {
        "ClusterId": "reclamations-cluster",
        "Match": {
          "Path": "/api/dashboard/{**catch-all}"
        }
      },
      "messages-route": {
        "ClusterId": "reclamations-cluster",
        "Match": {
          "Path": "/api/messages/{**catch-all}"
        }
      },
      "interventions-route": {
        "ClusterId": "interventions-cluster",
        "Match": {
          "Path": "/api/interventions/{**catch-all}"
        }
      },
      "technicians-route": {
        "ClusterId": "interventions-cluster",
        "Match": {
          "Path": "/api/technicians/{**catch-all}"
        }
      },
      "invoices-route": {
        "ClusterId": "interventions-cluster",
        "Match": {
          "Path": "/api/invoices/{**catch-all}"
        }
      },
      "orders-route": {
        "ClusterId": "articles-cluster",
        "Match": {
          "Path": "/api/orders/{**catch-all}"
        }
      },
      "uploads-route": {
        "ClusterId": "auth-cluster",
        "Match": {
          "Path": "/uploads/{**catch-all}"
        }
      }
    },
    "Clusters": {
      "auth-cluster": {
        "Destinations": {
          "auth-service": {
            "Address": "http://localhost:5001"
          }
        },
        "HealthCheck": {
          "Active": {
            "Enabled": true,
            "Interval": "00:00:30",
            "Timeout": "00:00:10",
            "Path": "/health"
          }
        }
      },
      "articles-cluster": {
        "Destinations": {
          "articles-service": {
            "Address": "http://localhost:5002"
          }
        },
        "HealthCheck": {
          "Active": {
            "Enabled": true,
            "Interval": "00:00:30",
            "Timeout": "00:00:10",
            "Path": "/health"
          }
        }
      },
      "reclamations-cluster": {
        "Destinations": {
          "reclamations-service": {
            "Address": "http://localhost:5003"
          }
        },
        "HealthCheck": {
          "Active": {
            "Enabled": true,
            "Interval": "00:00:30",
            "Timeout": "00:00:10",
            "Path": "/health"
          }
        }
      },
      "interventions-cluster": {
        "Destinations": {
          "interventions-service": {
            "Address": "http://localhost:5004"
          }
        },
        "HealthCheck": {
          "Active": {
            "Enabled": true,
            "Interval": "00:00:30",
            "Timeout": "00:00:10",
            "Path": "/health"
          }
        }
      }
    }
  }
}

